package praktikum5;
//inheritance (Pewarisan)

public abstract class Hewan {
	protected static String name;
	protected abstract void eat();
}
class Cat extends Hewan {
	public Cat(String namacat) {
		Cat.name = namacat;
	}
	public void eat () {
		System.out.print(Cat.name + " Makan Wishkas ");
	}
}
